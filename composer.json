{
    "name": "pfazzi/simplex-mapper",
    "description": "A simple mapping library",
    "type": "library",
    "authors": [
        {
            "name": "pfazzi",
            "email": "patrick91@live.it"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Pfazzi\\SimplexMapper\\": "src/"
        }
